{
  "name": "ui5-app-webcomponents",
  "version": "0.0.1",
  "description": "UI5 application w/ UI5 Web Components as UI5 controls",
  "private": true,
  "scripts": {
    "start": "ui5 build self-contained && ui5 serve --config ui5-dist.yaml --port 9082",
    "start:dev": "ui5 serve --port 8082"
  },
  "dependencies": {
    "@openui5/sap.f": "*",
    "@openui5/sap.m": "*",
    "@openui5/sap.ui.core": "*",
    "@openui5/sap.ui.layout": "*",
    "@openui5/themelib_sap_fiori_3": "*",
    "ui5-webcomponents-lib": "0.0.1",
    "ui5-myother-lib": "0.0.1"
  },
  "devDependencies": {
    "@ui5/cli": "*",
    "ui5-middleware-livereload": "*",
    "ui5-middleware-simpleproxy": "*"
  },
  "ui5": {
    "dependencies": [
      "@openui5/sap.f",
      "@openui5/sap.m",
      "@openui5/sap.ui.core",
      "@openui5/sap.ui.layout",
      "@openui5/themelib_sap_fiori_3",
      "ui5-middleware-livereload",
      "ui5-middleware-simpleproxy",
      "ui5-webcomponents-lib",
      "ui5-myother-lib"
        ]
  }
}
